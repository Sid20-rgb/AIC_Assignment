    <!-- ############################ -->
    <!-- QUIZ OBJECT PROPERTIES ADDED -->
    <!-- ############################ -->

    <owl:ObjectProperty rdf:about="http://www.example.com/ITS#hasQuiz">
        <rdfs:domain rdf:resource="http://www.example.com/ITS#Shape"/>
        <rdfs:range rdf:resource="http://www.example.com/ITS#Quiz"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="http://www.example.com/ITS#hasQuizQuestion">
        <rdfs:domain rdf:resource="http://www.example.com/ITS#Quiz"/>
        <rdfs:range rdf:resource="http://www.example.com/ITS#QuizQuestion"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="http://www.example.com/ITS#questionFromQuiz">
        <rdfs:domain rdf:resource="http://www.example.com/ITS#QuizQuestion"/>
        <rdfs:range rdf:resource="http://www.example.com/ITS#Quiz"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="http://www.example.com/ITS#hasQuizAttempt">
        <rdfs:domain rdf:resource="http://www.example.com/ITS#Student"/>
        <rdfs:range rdf:resource="http://www.example.com/ITS#QuizAttempt"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="http://www.example.com/ITS#attemptedQuizBy">
        <rdfs:domain rdf:resource="http://www.example.com/ITS#QuizAttempt"/>
        <rdfs:range rdf:resource="http://www.example.com/ITS#Student"/>
    </owl:ObjectProperty>


    <!-- ########################################################
         DATA PROPERTIES (Original + Quiz Added)
    ######################################################### -->

    <owl:DatatypeProperty rdf:about="http://www.example.com/ITS#formulaText">
